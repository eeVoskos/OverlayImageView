<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="OverlayImageView">
        <attr name="overlay_src" format="reference" />
        <attr name="overlay_padding" format="dimension" />
        <attr name="overlay_gravity">
            <enum name="center" value="0" />
            <enum name="top_left" value="1" />
            <enum name="top_right" value="2" />
            <enum name="bottom_left" value="3" />
            <enum name="bottom_right" value="4" />
        </attr>
    </declare-styleable>

</resources>